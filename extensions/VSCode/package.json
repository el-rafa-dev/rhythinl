{
  "name": "rhythin",
  "displayName": "Rhythin Syntax Highlights",
  "description": "Syntax Highlighter for Rhythin Programming Language",
  "publisher": "Rafael",
  "icon": "icon.png",
  "repository": {
    "url": "https://github.com/el-rafa-dev/rhythinl"
  },
  "version": "0.0.0-1",
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "rhythin",
      "aliases": ["Rhythin", ".ry"],
      "extensions": [".ry"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "rhythin",
      "scopeName": "source.rhythin",
      "path": "./syntaxes/.ry.tmLanguage.json"
    }]
  }
}
